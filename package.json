  "author": "Genadi Sokolov [genadis.pub@gmail.com]",
  "devDependencies": {
    "@types/jquery": "^3.3.30",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.11",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.18.2",
    "gulp": "^4.0.2",
    "gulp-awspublish": "^4.0.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^2.28.0",
    "node-sass": "^4.14.1",
    "require-dir": "^0.3.2",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.7",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-loader": "^3.5.4",
    "typescript": "^2.9.2",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.11.5",
    "webpack-merge": "^4.2.1",
    "yargs": "^13.3.0"
  },
  "engines": {
    "node": ">=12.0"
  },
  "main": "dist/plugin.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Annoto/kaltura-plugin.git"
  },
  "scripts": {
    "start": "webpack-dev-server --env.type=dev --progress --profile --colors",
    "start-https": "webpack-dev-server --env.type=dev --progress --profile --colors --env.https=1 --https",
    "start-staging": "webpack-dev-server --env.type=dev_staging --progress --profile --colors --https",
    "start:prod": "webpack-dev-server --env.type=dev_prod --progress --profile --colors",
    "build:prod": "webpack --env.type=prod --progress --profile --colors",
    "build": "webpack --env.type=staging --progress --profile --colors",
    "deploy": "gulp publish",
    "deploy:prod": "gulp publish --prod",
    "build:dev": "webpack --env.type=dev --progress --profile --colors",
    "build:dev-https": "webpack --env.type=dev --env.https=1 --progress --profile --colors",
    "prep:major": "npm run build && npm version major",
    "prep:minor": "npm run build && npm version minor",
    "prep:patch": "npm run build && npm version patch"
  },
  "dependencies": {
    "@annoto/widget-api": "^1.10.1"
  }
}
